{
  "expo": {
    "name": "Getaways Management",
    "slug": "Getaways-Management",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you upload photos when its required.",
          "cameraPermission": "The app accesses your device camera to let you upload photos when its required."
        }
      ]
    ],
    "version": "1.2.3",
    "orientation": "portrait",
    "icon": "./app/assets/ios_app_icon.png",
    "splash": {
      "image": "./app/assets/splash_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#a593a0"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "bundleIdentifier": "com.nospipi.GetawaysManagement",
      "buildNumber": "1.2.3",
      "icon": "./app/assets/ios_app_icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/getaways_adaptive_icon.png",
        "backgroundColor": "#edeae6"
      },
      "package": "com.nospipi.GetawaysManagement",
      "versionCode": 23,
      "googleServicesFile": "./google-services.json",
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./app/assets/getaways_adaptive_icon.png"
    },
    "description": ""
  }
}
